{
  "manifest_version": 2,
  "name": "house-hidden",
  "description": "house-hidden",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "jquery.min.js",
    "houseprice.js",
    "style.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://buy.houseprice.tw/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "execute.js"
      ],
      "run_at": "document_end"
    }
  ]
}